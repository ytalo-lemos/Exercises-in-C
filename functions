int main() {
    //q48
    int t = 0, v;
    float PE = 0, temp;
    for (int i = 1; ; i++) {
        printf("Insira o Preco da Etiqueta (ou 0 para sair): ");
        scanf("%f", &temp);
        if (temp == 0) {
            break;
        }
        PE += temp;
    }
    printf("\t1 - A vista em dinheiro ou cheque, com 10%% de desconto\n");
    printf("\t2 - A vista com cartao de credito, com 5%% de desconto\n");
    printf("\t3 - Em 2 vezes, preco normal de etiqueta sem juros\n");
    printf("\t4 - Em 3 vezes, preco de etiqueta com acrescimo de 10%%\n");
    scanf("%d", &v);
    switch(v) {
        case 1:
            t = PE * 0.1;
            PE = PE - t;
            printf("Valor total a ser pago usando dinheiro ou cheque: %.2f\n", PE);
            break;
        case 2:
            t = PE * 0.05;
            PE = PE - t;
            printf("Valor total a ser pago usando cartao de credito: %.2f\n", PE);
            break;
        case 3:
            printf("Valor a ser pago em 2x sem juros: %.2f\n", PE / 2);
            break;
        case 4:
            t = PE * 0.1;
            PE = PE + t;
            printf("Valor a ser pago em 3x com juros de 10%%: %.2f\n", PE / 3);
            break;
        default:
            printf("Valor invalido!\n");
            break;
    }
    return 0;
}
int main() {
    //q49
    int nh;
    float salario, maiorSalario = 0, totalSalarios = 0, mediaSalario, mediaFilhos, percentualSalario1000;
    int filhos, totalFilhos = 0, countSalario1000 = 0;
    printf("Numero de habitantes: ");
    scanf("%d", &nh);
    for (int i = 1; i <= nh; i++) {
        printf("Salario do habitante n%c%d: ",248,i);
        scanf("%f", &salario);
        printf("Numero de filhos do habitante n%c%d: ",248,i);
        scanf("%d", &filhos);
        totalSalarios += salario;
        if (salario > maiorSalario) {
            maiorSalario = salario;
        }
        totalFilhos += filhos;
        if (salario > 1000) {
            countSalario1000++;
        }
    }
    mediaSalario = totalSalarios / nh;
    mediaFilhos = (float)totalFilhos / nh;
    percentualSalario1000 = (float)countSalario1000 / nh * 100;
    printf("\nMedia do salario da populacao: %.2f", mediaSalario);
    printf("\nMedia do numero de filhos: %.0f", mediaFilhos);
    printf("\nMaior salario: %.2f", maiorSalario);
    printf("\npessoas com salarios ate $1000: %.1f%%\n", percentualSalario1000);
    return 0;
}
int main() {
    //q50
    int n = 15;
    char sexo;
    float altura, maiorAltura = 0, menorAltura = 999, totalAlturaMulheres = 0, mediaAlturaMulheres;
    int numHomens = 0;
    for (int i = 1; i <= n; i++) {
        printf("Sexo da pessoa %d (M/F): ", i);
        scanf(" %c", &sexo);
        printf("Altura da pessoa %d: ", i);
        scanf("%f", &altura);
        if (altura > maiorAltura) {
            maiorAltura = altura;
        }
        if (altura < menorAltura) {
            menorAltura = altura;
        }
        if (sexo == 'F' || sexo == 'f') {
            totalAlturaMulheres += altura;
        }
        if (sexo == 'M' || sexo == 'm') {
            numHomens++;
        }
    }
    mediaAlturaMulheres = totalAlturaMulheres / n;
    printf("\nMaior altura do grupo: %.2f\n", maiorAltura);
    printf("Menor altura do grupo: %.2f\n", menorAltura);
    printf("Media de altura das mulheres: %.2f\n", mediaAlturaMulheres);
    printf("Numero de homens: %d\n", numHomens);
    return 0;
}
int main() {
    //q51
    char sexo, resposta;
    int numSim = 0, numNao = 0, numMulheresSim = 0, numHomensNao = 0, n = 20;
    for (int i = 1; i <= n; i++) {
        printf("Sexo da pessoa %d (M/F): ", i);
        scanf(" %c", &sexo);

        printf("Resposta da pessoa %d (S/N): ", i);
        scanf(" %c", &resposta);

        if (resposta == 'S' || resposta == 's') {
            numSim++;
            if (sexo == 'F' || sexo == 'f') {
                numMulheresSim++;
            }
        } else if (resposta == 'N' || resposta == 'n') {
            numNao++;
            if (sexo == 'M' || sexo == 'm') {
                numHomensNao++;
            }
        }
    }

    float porcMulheresSim = (float)numMulheresSim / (n / 2) * 100;
    float porcHomensNao = (float)numHomensNao / (n / 2) * 100;

    printf("\nNumero de pessoas que responderam sim: %d\n", numSim);
    printf("Numero de pessoas que responderam nao: %d\n", numNao);
    printf("Porcentagem de mulheres que responderam sim: %.2f%%\n", porcMulheresSim);
    printf("Porcentagem de homens que responderam nao: %.2f%%\n", porcHomensNao);
    return 0;
}
