int main() {
    //q39
    int n = 10;
    int a = 0, b = 1, next;
    printf("Numero de termos: ");
    scanf("%d",&n);

    printf("Os termos da sequencia de Fibonacci sao:\n");

    for (int i = 1; i <= n; i++) {
        if (i == 1) {
            printf("%d ", a);
            continue;
        }
        if (i == 2) {
            printf("%d ", b);
            continue;
        }
        next = a + b;
        a = b;
        b = next;
        printf("%d ", next);
    }
    return 0;
}
