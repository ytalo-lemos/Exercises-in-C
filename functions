int main()
{
    //q31
    int v;
    float f,c;
    printf("\t1_conversao\n\t2_tabela\n\t");
    scanf("%d",&v);
    switch(v){
        case 1:
            printf("Digite os graus Fahrenheit: ");
            scanf("%f",&f);
            c = (f-32)*5/9;
            printf("valor em celsius: %.2f",c);
            c = 0;
            f = 0;
            break;
        case 2:
            c=50;
            for( int i=0;i<16;i++){
                f = (c+32)*9/5;
                printf("\n%.2f\n",c);
                printf("%.2f\n",f);
                c++;
            }
            break;
        default:
            printf("Valor invalido!");
            break;
        }
    return 0;
}
