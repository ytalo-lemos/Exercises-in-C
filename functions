int main()
{
    //q20
    int a,b,c,t=0;
    printf("Digite tres valores: ");
    scanf("%d%d%d",&a,&b,&c);
    if (a>b){
        t=a;
        a=b;
        b=t;
        t=0;
    }
    if (a>c){
        t=a;
        a=c;
        c=t;
        t=0;
    }
    if (b>c){
        t=b;
        b=c;
        c=t;
    }
    printf("Valores em ordem crescente: %d %d %d\n",a,b,c);
    return 0;
}
int main()
{
    //q21
    int a,b,c,ab,ac,bc;
    printf("Digite o valor de a b e c: ");
    scanf("%d%d%d",&a,&b,&c);
    ab=a+b;
    ac=a+c;
    bc=b+c;
    (ab<c || ac<b || bc<a) ? printf("Nao e um triangulo"):
        (a!=b || a!=c || c!=b) ? printf("Escaleno"):
            (a==b && b==c && a==c)? printf("Equilatero!"): printf("Isoscele");
    return 0;
}
int main()
{
    //q22
    int a,b,sm,sb,mp,dv,v,r;
    printf("\n\tDigite dois valores: ");
    scanf("%d%d",&a,&b);
    printf("\n\t1_adicao\n\n\t2_subtracao\n\n\t3_multiplicacao\n\n\t4_divisao\n\n\t");
    scanf("%d",&v);
    switch(v){
        case 1:
            sm=a+b;
            printf("\n\t%d\n",sm);
            break;
        case 2:
            sb=a-b;
            printf("\n\t%d\n",sb);
            break;
        case 3:
            mp=a*b;
            printf("\n\t%d\n",mp);
            break;
        case 4:
            dv=a/b;
            r=a%b;
            printf("\n\t%d Resto: %d\n",dv,r);
            break;
        default:
            printf("\n\tValor invalido!");
            break;
        }
    return 0;
}
int main()
{
    //23
    float a,b,c,d,r1,r2;
    printf("Digite os valores de a b e c da sua funcao: ");
    scanf("%f%f%f",&a,&b,&c);
    d = (b*b)-4*a*c;
    d = sqrt(d);
    (d>0) ? printf("ha duas solucoes reais e diferentes!\n") :
        (d<0) ? printf("nao ha solucao real!\n") : printf("ha duas solucoes reais e diferentes!\n");
    r1 = (-b+d);
    r1=r1/2*a;
    r2 = (-b-d);
    r2=r2/2*a;

    printf("%.2f\n%.2f",r1,r2);
    return 0;
}
