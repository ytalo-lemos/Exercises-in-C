// switch - case // menu
int main()
{
int v;
    printf("Insira um calor de 1 a 7: \n");
    scanf("%d",&v);
    switch(v){
        case 1:
            printf("Case_1");
            break;
        case 2:
            printf("Case_2");
            break;
        case 3:
            printf("Case_3");
            break;
        case 4:
            printf("Case_4");
            break;
        case 5:
            printf("Case_5");
            break;
        case 6:
            printf("Case_6");
            break;
        case 7:
            printf("Case_7");
            break;
        default:
            printf("Valor invalido!");
            break;
        }
    return 0;
}
