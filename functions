        printf("Voce digitou um numero entre 100 e 200 \n");
        printf("Numero valido %d\n",a);
    }
    if (b>=100 && b<=200){
        printf("Voce digitou um numero entre 100 e 200 \n");
        printf("Numero valido %d\n",b);
    }
    if (c>=100 && c<=200){
        printf("Voce digitou um numero entre 100 e 200 \n");
        printf("Numero valido %d\n",c);
    }
    if (d>=100 && d<=200){
        printf("Voce digitou um numero entre 100 e 200 \n");
        printf("Numero valido %d\n",d);
    }
    if (e>=100 && e<=200){
        printf("Voce digitou um numero entre 100 e 200 \n");
