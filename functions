    float fab,fin,imp,rev;
    setlocale(LC_ALL,"Portuguese");
    printf("\n\tInsira o valor de fabrica: ");
    scanf("%f",&fab);
    imp = fab * 45/100;
    rev = fab * 28/100;
    fin = fab + imp + rev;
    printf("\n\tO valor revendedor: R$%2.f\n",rev);
    printf("\n\tO valor imposto: R$%2.f\n",imp);
    printf("\n\tO preco final: R$%2.f\n",fin);
    return 0;
}
