int main()
{
    //q7
    setlocale(LC_ALL,"Portuguese");
    float a,v,pi,r,h,hr;
    pi = 3.14;
    printf("Digite o raio e a altura: ");
    scanf("%f%f",&r,&h);
    hr = h + r;
    v = pi * r*r * h;
    a = 2 * pi * r * hr;

    printf("A área do cilindro correspode a: %.2f\n",a);
    printf("O volume do cilindro correspode a: %.2f",v);
    return 0;
}
