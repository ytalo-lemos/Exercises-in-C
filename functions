int main()
{
    //q11
    setlocale(LC_ALL,"Portuguese");
    float valorc,valorp,troco;
    printf("\n\tDigite o valor da compra: ");
    scanf("%f",&valorc);
    printf("\n\tDigite o valor pago: ");
    scanf("%f",&valorp);
    troco = valorp - valorc;
    if (valorp < valorc)
        printf("\n\tFalta de pagamento!\n");
    else{
        if (valorp == valorc)
            printf("\n\tNao tem troco! \n");
        else
            printf("\n\tTroco: %.2f\n",troco);
    }
    return 0;
}
